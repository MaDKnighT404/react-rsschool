{
  "name": "2react-rsschool",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "ts-node --esm server.ts",
    "build": "npm run build:client && npm run build:server",
    "build:client": "vite build --ssrManifest --outDir dist/client",
    "build:server": "vite build --ssr src/entry-server.tsx --outDir dist/server",
    "preview": "vite preview",
    "lint": "npx eslint . --ext .ts,.tsx",
    "test": "vitest",
    "coverage": "vitest run --coverage",
    "cy:run": "cypress run",
    "cy:open": "cypress open",
    "cypress:console": "start-server-and-test dev http://localhost:3000 cy:run",
    "cypress:browser": "start-server-and-test dev http://localhost:3000 cy:open"
  },
  "dependencies": {
    "@hookform/resolvers": "^3.0.0",
    "@reduxjs/toolkit": "^1.9.3",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-hook-form": "^7.43.8",
    "react-icons": "^4.8.0",
    "react-redux": "^8.0.5",
    "react-router": "^6.9.0",
    "react-router-dom": "^6.9.0",
    "yup": "^1.0.2"
  },
  "devDependencies": {
    "@cypress/code-coverage": "^3.10.4",
    "@cypress/react18": "^2.0.0",
    "@istanbuljs/nyc-config-typescript": "^1.0.2",
    "@testing-library/dom": "^9.0.1",
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/react": "^14.0.0",
    "@testing-library/user-event": "^14.4.3",
    "@types/cypress": "^1.1.3",
    "@types/cypress__code-coverage": "^3.10.0",
    "@types/express": "^4.17.17",
    "@types/jest": "^29.4.1",
    "@types/react": "^18.0.27",
    "@types/react-dom": "^18.0.10",
    "@typescript-eslint/eslint-plugin": "^5.54.1",
    "@typescript-eslint/parser": "^5.54.1",
    "@vitejs/plugin-react": "^3.1.0",
    "@vitest/coverage-c8": "^0.29.2",
    "babel-plugin-istanbul": "^6.1.1",
    "cypress": "^12.10.0",
    "eslint": "^8.36.0",
    "eslint-config-prettier": "^8.7.0",
    "eslint-import-resolver-typescript": "^3.5.3",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-react": "^7.32.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "express": "^4.18.2",
    "istanbul-lib-coverage": "^3.2.0",
    "jest": "^29.5.0",
    "jsdom": "^21.1.1",
    "lint-staged": "^13.2.0",
    "nyc": "^15.1.0",
    "prettier": "^2.8.4",
    "sass": "^1.59.2",
    "start-server-and-test": "^2.0.0",
    "ts-node": "^10.9.1",
    "typescript": "^4.9.3",
    "vite": "^4.1.0",
    "vite-plugin-istanbul": "^4.0.1",
    "vitest": "^0.29.2"
  }
}
